{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 1,
    "no-plusplus": 0,
    "react/state-in-constructor": 0,
    "react/jsx-no-bind": 0,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": true
      }
    ]
  }
}
